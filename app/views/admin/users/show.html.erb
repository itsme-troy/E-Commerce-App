<h1><%= @user.email_address %></h1>
<p>Role: <%= @user.role %></p>

<div class="category-buttons">
  <%= link_to "Edit", edit_admin_user_path(@user), class: "btn edit" %>
  <%= button_to "Delete", admin_user_path(@user), 
              method: :delete, 
              data: { confirm: "Are you sure?" }, 
              class: "btn delete" %>
              
</div>
